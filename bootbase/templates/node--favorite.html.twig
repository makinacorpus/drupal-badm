<div id="node-{{ node.nid }}" class="{{ classes }} clearfix"{{ attributes }}>

  {{ user_picture }}

  {{ title_prefix }}
  {% if not page %}
    <h2{{ title_attributes }}><a href="{{ node_url }}">{{ title }}</a></h2>
  {% endif %}
  {{ title_suffix }}

  {% if display_submitted %}
    <div class="submitted">
      {{ submitted }}
    </div>
  {% endif %}

  <div class="content"{{ content_attributes }}>
    {{ content|without(['links','comments']) }}
  </div>

  {{ content.links }}
  {{ content.comments }}
</div>
